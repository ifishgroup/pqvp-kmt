{
  "name": "playground",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "Dustin Lish <dlish@ifishgroup.com>;Ernie Lopez <elopez@ifishgroup.com>",
  "private": true,
  "scripts": {
    "client": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "server": "npm start --prefix ./api",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "debug-api": "node --nolazy --inspect-brk=5858 ./api/server.js",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js",
    "e2e": "node test/e2e/runner.js",
    "e2e-docker": "node test/e2e/runner.js --config test/e2e/docker.nightwatch.conf.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "lint:fix": "yarn lint --fix",
    "lint-api": "eslint api/app",
    "lint-api:fix": "yarn lint-api --fix",
    "build": "node build/build.js",
    "build-all": "npm run build-insight && npm run build-api && npm run build-import",
    "build-insight": "docker build -t ifishgroup/insight:dev .",
    "build-api": "docker build -t ifishgroup/insight-api:dev ./api/",
    "build-import": "docker build -t ifishgroup/insight-import:dev ./api/data/",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "docker-e2e": "docker-compose -f docker-compose-e2e.yml -f docker-compose.yml run --rm e2e",
    "pull": "docker pull ifishgroup/insight && docker pull ifishgroup/insight-api && docker pull ifishgroup/insight-import"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^4.0.0",
    "chart.js": "^2.7.2",
    "concurrently": "^3.5.1",
    "i": "^0.3.6",
    "liquor-tree": "^0.2.11",
    "lodash": "^4.17.5",
    "markdown-it": "^8.4.1",
    "markdown-it-classy": "^0.2.0",
    "moment": "^2.21.0",
    "sanitize-html": "^1.18.2",
    "simplemde": "^1.11.2",
    "vee-validate": "^2.0.4",
    "vue": "^2.5.2",
    "vue-chartjs": "^3.2.1",
    "vue-nav-tabs": "^0.5.6",
    "vue-router": "^3.0.1",
    "vue-simplemde": "^0.4.6",
    "vue-social-sharing": "^2.3.3",
    "vue-toastr": "^2.0.12",
    "vuex": "^3.0.1",
