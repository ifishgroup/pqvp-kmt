{
  "swagger": "2.0",
  "info": {
    "description": "OpenAPI information about Insight KMT Api",
    "title": "Insight KMT Api",
    "version": "1.0.0"
  },
  "host": "localhost:3003",
  "schemes": [
    "http"
  ],
  "basePath": "/api",
  "paths": {
    "/users/create": {
      "post": {
        "tags": [
          "users"
        ],
        "summary": "Create User",
        "produces": [
          "application/json"
        ],
        "parameters": [{
            "in": "body",
            "name": "body",
            "description": "create a new Insight KMT user",
            "required": true,
            "schema": {
              "$ref": "#/definitions/User"
            }
          },
          {
            "name": "x-auth",
            "in": "header",
            "description": "an authorization jwt token in header",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          },
          "5XX": {
            "description": "Unexpected error"
          }
        }
      }
    },
    "/users/login": {
      "post": {
        "tags": [
          "users"
        ],
        "summary": "Login User",
        "produces": [
          "application/json"
        ],
        "parameters": [{
          "in": "body",
          "name": "body",
          "description": "credentials sent to login a user",
          "required": true,
          "schema": {
            "$ref": "#/definitions/User"
          }
        }],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/ReturnUserJson"
            },
            "headers": {
              "x-auth": {
                "type": "string",
                "description": "valid jwt token"
              }
            }
          },
          "400": {
            "description": "Invalid username/password supplied"
          },
          "401": {
            "description": "Unauthorized"
          },
          "5XX": {
            "description": "Unexpected error"
          }
        }
      }
    },
    "/users/edit/{id}": {
      "get": {
        "tags": [
          "users"
        ],
        "summary": "Retreive specific user profile by id",
        "produces": [
          "application/json"
        ],
        "parameters": [{
          "in": "path",
          "name": "id",
          "description": "string id of the user to retreive",
          "required": true,
          "schema": {
            "type": "string"
          }
        }],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/ReturnUserJson"
            },
            "401": {
              "description": "Unauthorized"
            },
            "5XX": {
              "description": "Unexpected error"
            }
          }
        }
      },
     "post": {
        "tags": [
          "users"
        ],
        "summary": "Update specific user profile by id",
        "produces": [
          "application/json"
        ],
        "parameters": [{
          "in": "path",
          "name": "id",
          "description": "string id of the user to retreive; Currently not being used.",
          "required": true,
          "schema": {
            "type": "string"
          }
        },
        {
          "in": "body",
          "name": "body",
          "description": "user profile to update",
          "required": true,
          "schema": {
            "$ref": "#/definitions/ReturnUserJson"
          }
        }
      ],
        "responses": {
          "200": {
            "description": "OK",
            "401": {
              "description": "Unauthorized"
            },
            "5XX": {
              "description": "Unexpected error"
            }
          }
        }
      }
    },
    "/users/delete/{id}": {
      "get": {
        "tags": [
          "users"
        ],
        "summary": "Delete User and return updated user list",
        "produces": [
          "application/json"
        ],
        "parameters": [{
          "in": "path",
          "name": "id",
          "description": "string id of the user to delete",
          "required": true,
          "schema": {
            "type": "string"
          }
        }],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/ReturnUserJson"
            },
            "headers": {
              "x-auth": {
                "type": "string",
                "description": "valid jwt token"
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "5XX": {
            "description": "Unexpected Error"
          }
        }
      }
    },
    "/users/all": {
      "get": {
        "tags": [
          "users"
        ],
        "summary": "Get list of all users",
        "produces": [
          "application/json"
        ],
        "parameters": [{
          "name": "x-auth",
          "in": "header",
          "description": "an authorization jwt token in header",
          "required": true,
          "type": "string"
        }],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/ReturnUserJson"
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "5XX": {
            "description": "Unexpected Error"
          }
        }
      }
    },
    "/users/me": {
      "get": {
        "tags": [
          "users"
        ],
        "summary": "Get user information for logged in user",
        "produces": [
          "application/json"
        ],
        "parameters": [{
          "name": "x-auth",
          "in": "header",
          "description": "an authorization jwt token in header",
          "required": true,
          "type": "string"
        }],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/ReturnUserJson"
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "5XX": {
            "description": "Unexpected Error"
          }
        }
      }
    },
    "/users/logout": {
      "delete": {
        "tags": [
          "users"
        ],
        "summary": "Logs out current logged in user session",
        "produces": [
          "application/json"
        ],
        "parameters": [{
          "name": "x-auth",
          "in": "header",
          "description": "an authorization jwt token in header",
          "required": true,
          "type": "string"
        }],
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Unauthorized"
          },
          "5XX": {
            "description": "Unexpected Error"
          }
        }
      }
    }
  },
